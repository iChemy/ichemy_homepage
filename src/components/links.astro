---
type MyLink = {
    name: string;
    url: string;
    account?: string;
};

const links: Array<MyLink> = [
    {
        name: "GitHub",
        url: "https://github.com/iChemy",
        account: "iChemy",
    },
    {
        name: "X",
        url: "https://x.com/iChemyee",
        account: "@iChemyee",
    },
];
---

<div>
    <ul>
        {
            links.map((link) => (
                <li>
                    <div>
                        <a href={link.url}>{link.name}</a>
                    </div>
                </li>
            ))
        }
    </ul>
</div>
